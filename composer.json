{    
    "authors": [
        {
            "homepage": "https://github.com/corentin-larose/",
            "name": "Corentin Larose",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "ZF\\HttpCache\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ZFTest\\HttpCache\\": "test/"
        }
    },
    "config": {
        "bin-dir" : "vendor/bin",
        "process-timeout" : 3600
    }, 
    "description": "ZF2 Module providing http cache headers support",
    "keywords": [
        "api",
        "apigility",
        "zf2",
        "zend",
        "module",
        "cache"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev",
            "dev-develop": "1.3-dev"
        }
    },
    "homepage": "http://apigility.org/",
    "license": "BSD-3-Clause",
    "name": "zfcampus/zf-http-cache",
    "repositories": [
        { "type": "composer", "url": "https://packages.zendframework.com" }
    ],
    "require": {
        "php": "^5.5|^7.0",
        "zendframework/zend-eventmanager": "~2.5",
        "zendframework/zend-http": "~2.5",
        "zendframework/zend-loader": "~2.5",
        "zendframework/zend-mvc": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "squizlabs/php_codesniffer": "^2.3.1"
    },
    "support": {
        "email": "apigility-users@zend.com",
        "irc": "irc://irc.freenode.net/apigility",
        "source": "https://github.com/zfcampus/zf-http-cache",
        "issues": "https://github.com/zfcampus/zf-http-cache/issues"
    },
    "type": "library"
}
